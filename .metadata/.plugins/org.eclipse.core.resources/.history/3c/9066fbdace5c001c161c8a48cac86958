package defecto;

import java.util.HashSet;

public class Lista {
	private HashSet<Persona> lista;

	public Lista(HashSet<Persona> lista) {
		lista = new HashSet<Persona>();
	}
	
	public int tamanyo() {
		return lista.size();
	}
	
	public boolean vacia() {
		return lista.isEmpty();
	}
}
